order = 0


def recursive_function1(order):
    if order == 10:
        return
    recursive_function1(order + 1)
    print("재귀함수를 호출합니다. %n", order)


def recursive_function2(order):
    if order == 10:
        return
    print("재귀함수를 호출합니다. %n", order)
    recursive_function2(order + 1)


recursive_function1(0)
print("=========================")
recursive_function2(0)
